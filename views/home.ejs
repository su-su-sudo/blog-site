<%- include('partials/header') -%>
<h1>Home</h1>

<p><%= startingContent %></p>
<% posts.forEach(function(p){ %>
  <h2><a href="/posts/<%= p._id %>" ><%= p.title%></h2></a>
  <p><%= p.content.substring(0, 30) + "..." %>
    <a href="/posts/<%= p._id %>" > Read More</a>
  </p>
<% }); %>


